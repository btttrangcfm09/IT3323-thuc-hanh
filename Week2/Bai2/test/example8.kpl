Program Example8; // Test các tính năng mới

// Khai báo biến với các kiểu dữ liệu mới
Var 
  msg : String;
  data : Bytes;
  x, y, result : Integer;

Begin
  // Test toán tử lấy phần dư %
  x := 10;
  y := 3;
  result := x % y;  // result = 1
  
  // Test toán tử lấy mũ **
  x := 2;
  y := 10;
  result := x ** y;  // result = 1024 (2^10)
  
  // Test chuỗi string
  msg := "Hello World";
  
  // Test cấu trúc REPEAT-UNTIL
  x := 0;
  Repeat
    x := x + 1;
    Call WriteLn;
  Until x = 5;
  
  (* Comment kiểu cũ vẫn hoạt động *)
  // Comment dạng dòng mới
  
  result := (5 + 3) ** 2 % 7;  // Kết hợp các toán tử
End. // Kết thúc chương trình
